{
  "presets": ["next/babel"],
  "plugins": [
    ["styled-components", { "ssr": true }],
    [
      "import",
      {
        "libraryName": "antd",
        "style": "css"
      },
      "antd"
    ],
    [
      "import",
      {
        "libraryName": "formik-antd",
        "style": "css"
      },
      "formik-antd"
    ],
    [
      "module-resolver",
      {
        "root": ["."],
        "alias": {
          "@modules": "./modules",
          "@store": "./store",
          "@lib": "./lib"
        }
      }
    ]
  ]
}
